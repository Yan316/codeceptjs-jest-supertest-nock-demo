{
  "output": "./ui-mobile/output",
  "helpers": {
    "Appium": {
      "app": "",
      "platform": "",
      "desiredCapabilities": {}
    },
    "ResembleHelper": {
      "require": "codeceptjs-resemblehelper",
      "screenshotFolder": "./ui-mobile/output/",
      "baseFolder": "./ui-mobile/screenshots/base/",
      "diffFolder": "./ui-mobile/screenshots/diff/"
    }
  },
  "include": {
